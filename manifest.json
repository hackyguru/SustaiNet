{
    "name": "SustaiNet",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "run_at": "document_start",
            "js": ["content.js"],
            "css": ["content.css"]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "sustainet"
    },
    "permissions": [
        "storage"
    ]
}